<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="regis" prefHeight="200.0" prefWidth="191.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginAndRegisterController">
   <children>
      <TextField fx:id="loginTextField" promptText="login" text="zabelko" />
      <PasswordField fx:id="passwordField" promptText="haseło" text="123123" />
      <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginUser" text="Zaloguj" />
      <Button fx:id="registerButton" mnemonicParsing="false" onAction="#openRegisterWindow" text="Stwórz konto" />
   </children>
</VBox>
